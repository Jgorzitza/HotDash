{
  "name": "llama-workflow",
  "version": "1.0.0",
  "description": "LlamaIndex-based AI workflow for HotDash knowledge ingestion",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "ingest:test": "tsx src/dev/test-ingest.ts",
    "refresh": "node --env-file=../../../.env.local --enable-source-maps --no-warnings dist/cli.js refresh",
    "query": "node --env-file=../../../.env.local --enable-source-maps --no-warnings dist/cli.js query",
    "insight": "node --env-file=../../../.env.local --enable-source-maps --no-warnings dist/cli.js insight",
    "eval": "node --env-file=../../../.env.local --enable-source-maps --no-warnings dist/eval/run.js"
  },
  "keywords": [
    "llamaindex",
    "ai",
    "rag",
    "workflow"
  ],
  "author": "HotDash AI Agent",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "llamaindex": "^0.12.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",
    "@types/uuid": "^10.0.0",
    "natural": "^8.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
